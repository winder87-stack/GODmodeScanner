{
  "agent": {
    "name": "WalletProfiler",
    "version": "1.0.0",
    "description": "Profiles wallet behavior and reputation"
  },
  "reputation": {
    "enabled": true,
    "base_score": 0.5,
    "age_weight": 0.2,
    "volume_weight": 0.15,
    "success_weight": 0.25,
    "community_weight": 0.2,
    "incident_penalty": 0.3,
    "min_score_to_flag": 0.3
  },
  "sybil_detection": {
    "enabled": true,
    "similarity_threshold": 0.7,
    "min_cluster_size": 3,
    "funding_pattern_weight": 0.3,
    "timing_pattern_weight": 0.25,
    "behavior_pattern_weight": 0.25,
    "network_pattern_weight": 0.2
  },
  "behavior_tracking": {
    "enabled": true,
    "tracking_window_days": 30,
    "pattern_detection_threshold": 3,
    "anomaly_detection_enabled": true,
    "store_full_history": false
  },
  "profiling": {
    "profile_cache_ttl_seconds": 600,
    "update_interval_seconds": 60,
    "deep_analysis_threshold": 0.6,
    "max_profiles_in_memory": 1000
  },
  "memory": {
    "enabled": true,
    "store_profiles": true,
    "profile_importance": "medium"
  }
}